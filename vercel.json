{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.svg",
      "use": "@vercel/static"
    },
    {
      "src": "*.xml",
      "use": "@vercel/static"
    },
    {
      "src": "*.json",
      "use": "@vercel/static"
    },
    {
      "src": "sw.js",
      "use": "@vercel/static"
    },
    {
      "src": "offline.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/join/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/dashboard",
      "dest": "/public/app.html"
    },
    {
      "src": "/missions",
      "dest": "/public/app.html"
    },
    {
      "src": "/progress",
      "dest": "/public/app.html"
    },
    {
      "src": "/badges",
      "dest": "/public/app.html"
    },
    {
      "src": "/referrals",
      "dest": "/public/app.html"
    },
    {
      "src": "/profile",
      "dest": "/public/app.html"
    },
    {
      "src": "/settings",
      "dest": "/public/app.html"
    },
    {
      "src": "/premium",
      "dest": "/public/app.html"
    },
    {
      "src": "/login",
      "dest": "/public/login.html"
    },
    {
      "src": "/signup",
      "dest": "/public/signup.html"
    },
    {
      "src": "/favicon.svg",
      "dest": "/favicon.svg"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/sw.js",
      "dest": "/sw.js"
    },
    {
      "src": "/offline.html",
      "dest": "/offline.html"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  }
}
